<?xml version="1.0" encoding="utf-8"?>
<!--
NOTE: I had to remove usual scoping from this declaration.
    E.g. package="mycompany.spatialite_database_driver"
    won't work. Specifically, the scoped namespace prevents
    java code in other apps that wish to use this module
    from doing an 'import jsqlite;'
-->
<manifest
    package="spatialite_database_driver"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="25" />

</manifest>